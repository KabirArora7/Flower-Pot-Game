type Msg = Tick Float GetKeyState
          | Pick
          | Drop1
          | Drop2
         {- | PickA
          | Drop1A
          | Drop2A -}

type alias Model = { time : Float
                   , flower : UniqueItem 
                   , flower2: UniqueItem
                   }

type UniqueItem = InVase
    | InHand
    | InPlace


update msg model = case msg of
                     Tick t _ -> { model | time = t }
                     Pick -> { model | flower = InHand , flower2= InHand}
                     Drop1 -> { model | flower =  InVase, flower2=  InVase }
                     Drop2-> {model | flower =  InPlace , flower2=  InPlace}
                     
                  {-   Pick-> { }
                     Drop1-> { model | flower2=  InVase}
                     Drop2 ->  {model | flower2=  InPlace} -}
                     

init = { time = 0
       , flower = InPlace
       , flower2 = InPlace
        }

main = gameApp Tick { model = init, view = view, update = update, title = "Game Slot" }

view model = collage 192 128 (myShapes model)



